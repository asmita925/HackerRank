using namespace std;

void convertStringToInteger(string s)
{
   int i = 0, num = 0;

   try {
       num = stoi(s);
       cout<<num;
   } catch(exception e) {
       cout<<"Bad String";
   }
}

int main(){
   string S;
   cin >> S;

   convertStringToInteger(S);

   return 0;
}
