// https://www.hackerrank.com/challenges/30-generics 
// C++

#include <iostream>
#include <vector>
#include <string>

using namespace std;

/**
*    Name: printArray
*    Print each element of the generic vector on a new line. Do not return anything.
*    @param A generic vector
**/

template <typename T>
void printArray (vector<T> generic_vector)
{
   for (int i = 0; i < generic_vector.size(); i++)
   {
       cout<<generic_vector[i]<<endl;
   }
}
