//https://www.hackerrank.com/challenges/30-operators
// C++

#include <bits/stdc++.h>

using namespace std;

void solve(double meal_cost, int tip_percent, int tax_percent) {
   double total;
   total = (meal_cost+(meal_cost*tip_percent*0.01)+ (meal_cost*tax_percent*0.01));

   if (total - (int)total >= 0.50)
   
       cout<<(int)total+1;
   } else {
       cout<<(int)total;
}
