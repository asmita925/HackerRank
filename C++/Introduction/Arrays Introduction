// https://www.hackerrank.com/challenges/arrays-introduction
// C++
#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>

using namespace std;

int main() {
   int size = 0, i = 0;
   cin>>size;
   int arr[size];

   for (i = 0; i < size; i++)
   {
       cin>>arr[i];
   }
  
   for (i = size; i > 0; i--)
   {
       cout<<arr[i-1]<<" ";
   }

   return 0;
}
