// https://www.hackerrank.com/challenges/c-tutorial-basic-data-types
// C++

#include <iostream>
#include <cstdio>
using namespace std;

int main() {
   int num1 = 0;
   long num2 = 0;
   char ch;
   float num3 = 0.0;
   double num4 = 0.00;

   scanf("%d %ld %c %f %lf", &num1, &num2, &ch, &num3, &num4);
   printf("%d\n", num1);
   printf("%ld\n", num2);
   printf("%c\n", ch);
   printf("%f\n", num3);
   printf("%lf\n", num4);
       
   return 0;
}
