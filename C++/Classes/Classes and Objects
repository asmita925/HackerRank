// https://www.hackerrank.com/challenges/classes-objects
// C++

#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
#include <cassert>
using namespace std;

class Student {
   private:
       int scores[5];

   public:
       void input()
       {
           int i = 0;

           for (i = 0; i < 5; i++)
           {
               cin>>scores[i];
           }
       }

       int calculateTotalScore()
       {
           int i = 0, sum = 0;

           for (i = 0; i < 5; i++)
           {
               sum += scores[i];
           }

           return sum;
       }
};
